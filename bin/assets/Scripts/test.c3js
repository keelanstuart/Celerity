var runtime;
var hpos;

function update(elapsed_seconds)
{
	runtime += elapsed_seconds;
	var pos = GetPropertyValue(hpos);
	pos.x = Math.sin(runtime) * 10.0;
	pos.y = Math.cos(runtime) * 10.0;
	SetPropertyValue(hpos, pos);
}

function init()
{
	runtime = 0.0;

	hpos = FindProperty(self, "Position");
}